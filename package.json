{
  "name": "tfjs-ts-examples",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start-server": "node ./dist/server/index.js",
    "dev-server": "nodemon ./src/server/index.ts --watch ./src/server/**/*.ts",
    "dev-client": "parcel ./public/index.html",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tensorflow",
    "tfjs",
    "react",
    "typescript"
  ],
  "author": "nick nance",
  "license": "ISC",
  "devDependencies": {
    "@material/button": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@parcel/transformer-sass": "^2.8.0",
    "@types/cors": "^2.8.12",
    "@types/d3": "^7.4.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.0",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
